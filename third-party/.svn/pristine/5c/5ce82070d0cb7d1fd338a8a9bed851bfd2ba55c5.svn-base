<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="module_jakarta-commons" default="compile.module.jakarta-commons">
  <dirname property="module.jakarta-commons.basedir" file="${ant.file.module_jakarta-commons}"/>
  
  <property name="module.jdk.home.jakarta-commons" value="${project.jdk.home}"/>
  <property name="module.jdk.classpath.jakarta-commons" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.jakarta-commons" value="${compiler.args}"/>
  
  <property name="jakarta-commons.output.dir" value="${module.jakarta-commons.basedir}/build/classes"/>
  <property name="jakarta-commons.testoutput.dir" value="${module.jakarta-commons.basedir}/build/classes"/>
  
  <path id="jakarta-commons.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="jakarta-commons.module.classpath">
    <path refid="${module.jdk.classpath.jakarta-commons}"/>
    <pathelement location="${jakarta-commons.output.dir}"/>
    <pathelement location="${module.jakarta-commons.basedir}/commons-beanutils-1.6.1/commons-beanutils.jar"/>
    <pathelement location="${module.jakarta-commons.basedir}/commons-collections-2.1/commons-collections.jar"/>
    <pathelement location="${module.jakarta-commons.basedir}/commons-digester-1.5/commons-digester.jar"/>
    <pathelement location="${module.jakarta-commons.basedir}/commons-logging-1.0.3/commons-logging-api.jar"/>
    <pathelement location="${module.jakarta-commons.basedir}/commons-logging-1.0.3/commons-logging.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.jakarta-commons"/>
  
  <patternset id="excluded.from.compilation.jakarta-commons">
    <patternset refid="excluded.from.module.jakarta-commons"/>
    <patternset refid="compiler.excluded"/>
  </patternset>
  
  
  <target name="compile.module.jakarta-commons" depends="compile.module.jakarta-commons.production,compile.module.jakarta-commons.tests" description="compile module jakarta-commons"/>
  
  <target name="compile.module.jakarta-commons.production" description="compile module jakarta-commons production classes"/>
  
  <target name="compile.module.jakarta-commons.tests" depends="compile.module.jakarta-commons.production" description="compile module jakarta-commons test classes" unless="skip.tests"/>
  
  <target name="clean.module.jakarta-commons" description="cleanup module">
    <delete dir="${jakarta-commons.output.dir}"/>
    <delete dir="${jakarta-commons.testoutput.dir}"/>
  </target>
</project>